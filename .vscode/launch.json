    "version": "0.2.0",
    "configurations": [
 
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "bin/Debug/net8.0/Altinn.AccessManagement.dll",
            "cwd": "${workspaceFolder}/src/Altinn.AccessManagement",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
            },
            "env": {
                "PlatformSettings__ApiProfileEndpoint": "https://c4730895-3327-4fa4-aa20-f1535a5e3731.mock.pstmn.io/profile/api/v1/",
                "SblBridgeSettings__BaseApiUrl": "https://93dc697b-85ca-440a-8723-ffa1d85e5002.mock.pstmn.io/",
                "PlatformSettings__ApiRegisterEndpoint": "https://8cad6a5a-4e27-4513-a7a0-94914a3f2f68.mock.pstmn.io/register/api/v1/"
            },
            "launchSettingsProfile": "Altinn.AccessManagement",
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/Views"
            }
        }
    ]
}